{
  "manifest_version": 3,
  "name": "Twitch Message",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": "op.jpg"
  },
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://*.twitch.tv/*"]
}
